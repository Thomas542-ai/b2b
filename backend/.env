RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# UPI (for Indian payments)
UPI_MERCHANT_ID=your_upi_merchant_id
UPI_SECRET_KEY=your_upi_secret_key

# External APIs
# Google Maps
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Email Verification
NEVERBOUNCE_API_KEY=your_neverbounce_api_key
DEBOUNCE_API_KEY=your_debounce_api_key

